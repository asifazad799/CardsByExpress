

<section class="pt-5 mt-5 mb-5 pb-5 ">
    <div class="container mt-5 pt-5 pb-5 mb-5" >
        <div class="row justify-content-around">
            {{#each items}}
            <div class="col-12 col-sm-12 col-md-3 col-lg-3 d-flex justify-content-around pt-5 pb-5 mb-5 mt-5" style="background-color: rgb(124, 124, 124);">
                <div class="text-center pt-3" >
                    <span class="material-icons outlined" style="font-size: 50px;" >account_circle</span>
                    <h2>{{this.user}}</h2>
                    <p class="fs-4">age:{{this.age}}</p>
                    <address class="fs-4">place:{{this.place}}</address>
                </div>
             </div>
            {{/each}}
        </div>
    </div>
</section>
